# Editing configs
alias ev='vim ~/.vimrc'
alias et='vim ~/.tmux.conf'
alias ez='vim ~/.zshrc'
alias ea='vim ~/.oh-my-zsh/lib/alias.zsh'

# Change directory fast
alias cd1="cd .."
alias cd2="cd ../.."
alias cd3="cd ../../.."
alias cd4="cd ../../../.."
alias cd5="cd ../../../../.."

# My ip public
alias ippublica="curl ipv4.icanhazip.com"

# tmux profile alias
alias tmuxi="tmuxinator my_profile"

# Confirm remove files
alias rm="rm -v -i"
